<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="console"
              class="io.edap.log.appenders.ConsoleAppender">
        <encoder>
            <pattern>%date{yyyy-MM-dd HH:mm:ss.SSS} %-5level [%thread] [%logger{36}] [%X{traceId}] [%X{spanId}]  - %msg %ex%n</pattern>
        </encoder>
    </appender>
    <root level="info">
        <appender-ref ref="console" />
    </root>
</configuration>